        android:supportsRtl="true"
        android:theme="@style/Theme.Sonora">
        <activity
            android:name="chill.chillActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"
            android:exported="false" />
        <activity
            android:name="party.partyActivity"
